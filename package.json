  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build --stats-json",
    "analyze:portal": "webpack-bundle-analyzer dist/apps/portal/browser/stats.json",
    "test": "nx test",
    "deploy:api:prod": "export NODE_OPTIONS='--max-old-space-size=8192' && export NODE_ENV=production && sls deploy --stage production --config serverless-api.yml",
    "build:portal:prod": "export NODE_OPTIONS='--max-old-space-size=8192' && export NODE_ENV=production && nx run portal:build:production && nx run portal:server:production",
    "deploy:portal:prod": "export NODE_OPTIONS='--max-old-space-size=8192' && export NODE_ENV=production && nx run portal:build:production && nx run portal:server:production && sls deploy --stage production --config serverless-portal.yml",
    "deploy:api:prod:ncc": "export NODE_OPTIONS='--max-old-space-size=8192' && export NODE_ENV=production && npx ncc build ./apps/api/src/index.ts -o dist/apps/api && sls deploy --stage production --config serverless-api.yml",
    "deploy:portal:prod:ncc": "export NODE_OPTIONS='--max-old-space-size=8192' && export NODE_ENV=production && nx run portal:build:production && nx run portal:server:production && npx ncc build ./handler.js -o dist/apps/portal && sls deploy --stage production --config serverless-portal.yml"
  },
